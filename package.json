  "homepage": "https://github.com/khgame/jsonrpc#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "files": [
    "lib"
  ],
